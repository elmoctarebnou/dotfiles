{
	"vim.leader": "<space>",
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": false,
	"vim.hlsearch": true,
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false,
		"<C-p>": false
	},
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<Leader>", "/"],
			"commands": [":noh"]
		},
		{
			"before": ["<leader>", "vs"],
			"commands": ["workbench.action.splitEditor"]
		},
		{
			"before": ["<leader>", "e"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		},
		{
			"before": ["<Leader>", "p"],
			"commands": ["workbench.action.showCommands"]
		},
		{
			"before": ["<Leader>", "t"],
			"commands": ["workbench.action.gotoSymbol"]
		},
		{
			"before": ["<Leader>", "c", "c"],
			"commands": ["editor.action.commentLine"]
		},
		{
			"before": ["<leader>", "c", "u"],
			"commands": ["editor.action.blockcomment"]
		},
		// panes
		{
			"before": ["leader", "h"],
			"commands": ["workbench.action.focusLeftGroup"]
		},
		{
			"before": ["leader", "j"],
			"commands": ["workbench.action.focusBelowGroup"]
		},
		{
			"before": ["leader", "k"],
			"commands": ["workbench.action.focusAboveGroup"]
		},
		{
			"before": ["leader", "l"],
			"commands": ["workbench.action.focusRightGroup"]
		},
		// File save
		{ "before": ["leader", "w"], "commands": [":w!"] },
		{ "before": ["leader", "q"], "commands": [":q!"] },
		{ "before": ["leader", "x"], "commands": [":x!"] },
		/* move line up */
		{
			"before": ["<leader>", "u"],
			"commands": ["editor.action.moveLinesUpAction"]
		},
		/* move line down */
		{
			"before": ["<leader>", "d"],
			"commands": ["editor.action.moveLinesDownAction"]
		}
	],
	"vim.normalModeKeyBindings": [
		{
			"before": ["J"],
			"after": ["5", "j", "z", "z"]
		},
		{
			"before": ["K"],
			"after": ["5", "k", "z", "z"]
		},
		{
			"before": ["<leader>", "a"],
			"commands": ["workbench.view.explorer"]
		},
		{
			"before": ["g", "p", "d"],
			"commands": ["editor.action.peekDefinition"]
		},
		{
			"before": ["g", "h"],
			"commands": ["editor.action.showDefinitionPreviewHover"]
		},
		{
			"before": ["g", "i"],
			"commands": ["editor.action.goToImplementation"]
		},
		{
			"before": ["g", "p", "i"],
			"commands": ["editor.action.peekImplementation"]
		},
		{
			"before": ["q", "f"],
			"commands": ["editor.action.quickFix"]
		},
		{
			"before": ["s", "r"],
			"commands": ["editor.action.referenceSearch.trigger"]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			"before": ["J"],
			"after": ["5", "j", "z", "z"]
		},
		{
			"before": ["K"],
			"after": ["5", "k", "z", "z"]
		},
		{
			"before": [">"],
			"commands": ["editor.action.indentLines"]
		},
		{
			"before": ["<"],
			"commands": ["editor.action.outdentLines"]
		},
		// Commenting
		{
			"before": ["<leader>", "c", "c"],
			"commands": ["editor.action.commentLine"]
		}
	],
	"workbench.colorTheme": "Default Dark+",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"explorer.confirmDelete": false,
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"jupyter.askForKernelRestart": false,
	"explorer.confirmDragAndDrop": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.iconTheme": "vscode-icons",
	"bracketPairColorizer.depreciation-notice": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"indentRainbow.includedLanguages": [], // for example ["nim", "nims", "python"]
	// For which languages indent-rainbow should be deactivated (if empty it means none).
	"indentRainbow.excludedLanguages": ["plaintext"],
	// The delay in ms until the editor gets updated.
	"indentRainbow.updateDelay": 100,
	"[python]": {
		"editor.defaultFormatter": "ms-python.python",
		"python.defaultInterpreterPath": "/home/ubuntu/.local/share/virtualenvs/adams_backend-nueCwieJ/bin/python",
		"python.pythonPath": "/home/ubuntu/.local/share/virtualenvs/adams_backend-nueCwieJ/bin/python",
		"editor.formatOnType": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.tabs.location": "left",
	"git.terminalAuthentication": false,
	"git.enableSmartCommit": true,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"prettier.tabWidth": 4,
	"html.format.unformatted": "",
	"prettier.useTabs": true,
	"workbench.editor.wrapTabs": true,
	"notebook.output.textLineLimit": 100,
	"editor.minimap.enabled": false,
	"cSpell.userWords": [
		"allo",
		"astype",
		"bhac",
		"boto",
		"Cait",
		"cashmin",
		"cumsum",
		"cusip",
		"dailyquotes",
		"dataframe",
		"dataframes",
		"dotenv",
		"dropna",
		"Dtype",
		"fillna",
		"fundawaaccount",
		"FUNDCASH",
		"getbuffer",
		"householdaccountconnection",
		"idxmax",
		"idxmin",
		"iloc",
		"iterrows",
		"itertuples",
		"Langchain",
		"mcal",
		"modelassset",
		"mogrify",
		"nbbo",
		"OPENAI",
		"polygonio",
		"psycopg",
		"pytz",
		"rebal",
		"Rebalance",
		"registrationtype",
		"registrationtypes",
		"relativedelta",
		"schx",
		"SPLG",
		"TSLA",
		"Vectorizer",
		"wealthbox"
	],
	"editor.fontSize": 12.5,
	"jupyter.interactiveWindow.creationMode": "single",
	"editor.inlineSuggest.enabled": true,
	"githubPullRequests.pullBranch": "never",
	"remote.autoForwardPorts": false
}

